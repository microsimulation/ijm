{
  "name": "ijm-tests",
  "version": "1.0.0",
  "description": {
    "url": ""
  },
  "main": "",
  "scripts": {
    "build": "babel src --out-dir dist --copy-files",
    "test": "npm run build && cucumber-js dist/features/*.feature --require dist/features/step-definitions/*.js -f json:./reports/report.json -t @Run",
    "ci": "cucumber-js dist/features/*.feature --require 'dist/features/step-definitions/*.js' -f json:./reports/report.json -t @Run && npm run generate-html-report",
    "generate-html-report": "node ./report.js",
    "test-report": "npm run build && npm run test && npm run generate-html-report"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/microsimulation/ijm.git"
  },
  "author": "",
  "license": "MIT",
  "homepage": "",
  "dependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-inline-json-import": "0.3.2",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-util-promisify": "0.2.2",
    "babel-preset-env": "1.7.0",
    "chai": "4.2.0",
    "chromedriver": "^83.0.1",
    "cucumber": "^6.0.5",
    "cucumber-html-reporter": "^2.0.3",
    "geckodriver": "^1.1.3",
    "multiple-cucumber-html-reporter": "^1.16.3",
    "sanitize-filename": "^1.6.3",
    "selenium-webdriver": "^3.6.0"
  }
